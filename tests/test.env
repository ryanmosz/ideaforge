# Test environment configuration
# Shorter timeouts for tests to fail gracefully

# n8n webhook configuration for tests
N8N_BASE_URL=http://localhost:5678
N8N_WEBHOOK_PATH=webhook
N8N_API_KEY=test-api-key
N8N_TIMEOUT=10000  # 10 seconds instead of 30
N8N_RETRIES=1      # Fewer retries for tests

# Other test configurations
NODE_ENV=test
LOG_LEVEL=error 