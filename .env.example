# IdeaForge Environment Variables
# Copy this file to .env and fill in your values

# Required: OpenAI API Key for AI analysis  
OPENAI_API_KEY=your-openai-api-key-here

# Optional: n8n webhook URL for external research integration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/ideaforge

# Optional: Log level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Node environment (development, production, test)
NODE_ENV=development
